<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="fragments.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/my_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />


    </fragment>
    <fragment
        android:id="@+id/signupFragment"
        android:name="fragments.SignupFragment"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_signup" >
        <action
            android:id="@+id/action_signupFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_signupFragment_to_homeFragment"
            app:destination="@id/homeFragment" />

    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/my_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginFragment_to_signupFragment"
            app:destination="@id/signupFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_vaultFragment"
            app:destination="@id/vaultFragment" />
        <action
            android:id="@+id/action_homeFragment_to_appsFragment"
            app:destination="@id/appsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_chart"
            app:destination="@id/chart" />
        <action
            android:id="@+id/action_homeFragment_to_graphFragment"
            app:destination="@id/graphFragment" />

    </fragment>
    <fragment
        android:id="@+id/vaultFragment"
        android:name="fragments.VaultFragment"
        android:label="fragment_vault"
        tools:layout="@layout/fragment_vault" >
        <action
            android:id="@+id/action_vaultFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/my_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_vaultFragment_to_appsFragment"
            app:destination="@id/appsFragment" />
        <action
            android:id="@+id/action_vaultFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_vaultFragment_to_biometricFragment"
            app:destination="@id/biometricFragment" />

    </fragment>
    <fragment
        android:id="@+id/appsFragment"
        android:name="fragments.AppsFragment"
        android:label="fragment_apps"
        tools:layout="@layout/fragment_apps" >
        <action
            android:id="@+id/action_appsFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/my_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_appsFragment_to_vaultFragment"
            app:destination="@id/vaultFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/my_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_profileFragment_to_vaultFragment"
            app:destination="@id/vaultFragment" />
        <action
            android:id="@+id/action_profileFragment_to_appsFragment"
            app:destination="@id/appsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_resetPinFragment"
            app:destination="@id/resetPinFragment" />
        <action
            android:id="@+id/action_profileFragment_to_updateFragment"
            app:destination="@id/updateFragment" />
        <action
            android:id="@+id/action_profileFragment_to_biometricFragment"
            app:destination="@id/biometricFragment" />
        <action
            android:id="@+id/action_profileFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/my_nav"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_profileFragment_to_privacyFragment"
            app:destination="@id/privacyFragment" />
        <action
            android:id="@+id/action_profileFragment_to_termsFragment"
            app:destination="@id/termsFragment" />

    </fragment>
    <fragment
        android:id="@+id/chart"
        android:name="fragments.Chart"
        android:label="fragment_chart2"
        tools:layout="@layout/fragment_chart2" >
        <action
            android:id="@+id/action_chart_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/pinGenerationFragment"
        android:name="fragments.PinGenerationFragment"
        android:label="fragment_pin_generation"
        tools:layout="@layout/fragment_pin_generation" >
        <action
            android:id="@+id/action_pinGenerationFragment_to_biometricFragment"
            app:destination="@id/biometricFragment"
            app:popUpTo="@id/biometricFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/resetPinFragment"
        android:name="fragments.ResetPinFragment"
        android:label="fragment_reset_pin"
        tools:layout="@layout/fragment_reset_pin" >
        <action
            android:id="@+id/action_resetPinFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/updateFragment"
        android:name="fragments.UpdateFragment"
        android:label="fragment_update"
        tools:layout="@layout/fragment_update" >
        <action
            android:id="@+id/action_updateFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:popUpTo="@id/my_nav"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/biometricFragment"
        android:name="fragments.BiometricFragment"
        android:label="fragment_biometric"
        tools:layout="@layout/fragment_biometric" >
        <action
            android:id="@+id/action_biometricFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_biometricFragment_to_pinGenerationFragment"
            app:destination="@id/pinGenerationFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/graphFragment"
        android:name="fragments.GraphFragment"
        android:label="fragment_graph"
        tools:layout="@layout/fragment_graph" >
        <action
            android:id="@+id/action_graphFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/privacyFragment"
        android:name="fragments.PrivacyFragment"
        android:label="fragment_privacy"
        tools:layout="@layout/fragment_privacy" >
        <action
            android:id="@+id/action_privacyFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/termsFragment"
        android:name="fragments.TermsFragment"
        android:label="fragment_Terms"
        tools:layout="@layout/fragment_terms" >
        <action
            android:id="@+id/action_termsFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>


</navigation>